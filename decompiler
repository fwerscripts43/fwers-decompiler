local password = "FwersUncopylocks5534"
local attempts = 0

-- Create GUI
local gui = Instance.new("ScreenGui")
gui.Name = "fwers decompiler"
gui.Parent = game.CoreGui

-- Main Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 440, 0, 280)
frame.Position = UDim2.new(0.5, -220, 0.5, -140)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
frame.Parent = gui

local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 8)

-- Title
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 50)
title.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
title.Text = "Fwers Uncopylocks! DECOMPILER "
title.Font = Enum.Font.GothamBold
title.TextSize = 20
title.TextColor3 = Color3.new(1, 1, 1)
title.Parent = frame

local titleCorner = Instance.new("UICorner", title)
titleCorner.CornerRadius = UDim.new(0, 8)

-- Input
local input = Instance.new("TextBox")
input.Size = UDim2.new(1, -40, 0, 35)
input.Position = UDim2.new(0, 20, 0, 65)
input.PlaceholderText = "üîí Enter Password"
input.Text = ""
input.Font = Enum.Font.Gotham
input.TextSize = 16
input.TextColor3 = Color3.new(1, 1, 1)
input.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
input.ClearTextOnFocus = false
input.Parent = frame

local inputCorner = Instance.new("UICorner", input)
inputCorner.CornerRadius = UDim.new(0, 4)

-- Unlock Button
local button = Instance.new("TextButton")
button.Size = UDim2.new(1, -40, 0, 40)
button.Position = UDim2.new(0, 20, 0, 115)
button.Text = "üîì Unlock"
button.Font = Enum.Font.GothamBold
button.TextSize = 18
button.TextColor3 = Color3.new(1, 1, 1)
button.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
button.Parent = frame

local buttonCorner = Instance.new("UICorner", button)
buttonCorner.CornerRadius = UDim.new(0, 4)

-- Copy Game Button ‚Äî below Get Key
local copyButton = Instance.new("TextButton")
copyButton.Size = UDim2.new(1, -40, 0, 40)
copyButton.Position = UDim2.new(0, 20, 0, 205)  -- below Get Key
copyButton.Text = "üìÅ Copy Game + GUI + Scripts"
copyButton.Font = Enum.Font.GothamBold
copyButton.TextSize = 18
copyButton.TextColor3 = Color3.new(1, 1, 1)
copyButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
copyButton.Visible = false
copyButton.Parent = frame

local copyCorner = Instance.new("UICorner", copyButton)
copyCorner.CornerRadius = UDim.new(0, 4)

-- Get Key Button (Discord) ‚Äî positioned **under Unlock**
local linkButton = Instance.new("TextButton")
linkButton.Size = UDim2.new(1, -40, 0, 40)
linkButton.Position = UDim2.new(0, 20, 0, 160)  -- right under Unlock
linkButton.Text = "üîó Get Key"
linkButton.Font = Enum.Font.GothamBold
linkButton.TextSize = 18
linkButton.TextColor3 = Color3.new(1, 1, 1)
linkButton.BackgroundColor3 = Color3.fromRGB(255, 140, 0)
linkButton.Visible = false
linkButton.Parent = frame

local linkCorner = Instance.new("UICorner", linkButton)
linkCorner.CornerRadius = UDim.new(0, 4)

-- Status Label
local status = Instance.new("TextLabel")
status.Size = UDim2.new(1, -40, 0, 30)
status.Position = UDim2.new(0, 20, 1, -60)
status.BackgroundTransparency = 1
status.Text = "üîß Status: Waiting for password..."
status.Font = Enum.Font.Gotham
status.TextSize = 14
status.TextColor3 = Color3.fromRGB(180, 180, 180)
status.TextXAlignment = Enum.TextXAlignment.Left
status.Parent = frame

-- Footer
local footer = Instance.new("TextLabel")
footer.Size = UDim2.new(1, 0, 0, 30)
footer.Position = UDim2.new(0, 0, 1, -30)
footer.BackgroundTransparency = 1
footer.Text = "‚ö†Ô∏è 3 Attempts Only | made by Fwers Uncopylocks!"
footer.Font = Enum.Font.Gotham
footer.TextSize = 14
footer.TextColor3 = Color3.fromRGB(180, 180, 180)
footer.Parent = frame

-- Status fade function
local function showStatus(text)
    status.Text = text
    status.TextTransparency = 0
    spawn(function()
        wait(3)
        for i = 0, 1, 0.05 do
            status.TextTransparency = i
            wait(0.03)
        end
    end)
end

-- Unlock Button Logic
button.MouseButton1Click:Connect(function()
    if input.Text == password then
        title.Text = "‚úÖ Access Granted"
        showStatus("üîì Unlocked. Click a button below")
        button.Visible = false
        input.Visible = false
        copyButton.Visible = true
        linkButton.Visible = true
    else
        attempts += 1
        showStatus("‚ùå Wrong Password ("..attempts.."/3)")
        if attempts >= 3 then
            title.Text = "‚õî Too Many Failed Attempts"
            wait(1)
            gui:Destroy()
        end
    end
end)

-- Copy Game Logic
copyButton.MouseButton1Click:Connect(function()
    showStatus("üìÅ Copying game with scripts & UI...")
    local success, err = pcall(function()
        local StarterGui = game:GetService("StarterGui")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")

        -- Clone decompiler UI
        local cloneUI = gui:Clone()
        cloneUI.Parent = StarterGui

        -- Clone ReplicatedGUI folder
        local replicatedGUIs = ReplicatedStorage:FindFirstChild("ReplicatedGUI")
        if replicatedGUIs then
            for _, guiObj in pairs(replicatedGUIs:GetChildren()) do
                guiObj:Clone().Parent = StarterGui
            end
        end

        -- Save game
        local saveinstance = loadstring(game:HttpGet("https://raw.githubusercontent.com/luau/SynSaveInstance/main/saveinstance.luau"))()
        saveinstance({
            FileName = "Fwers Uncopylocks!"..game.PlaceId..".rbxlx",
            Decompile = true,
            IncludeScripts = true,
            CreatorTag = "Copied by Fwers Uncopylocks!"
        })

        -- Cleanup
        cloneUI:Destroy()
        if replicatedGUIs then
            for _, guiObj in pairs(StarterGui:GetChildren()) do
                if replicatedGUIs:FindFirstChild(guiObj.Name) then
                    guiObj:Destroy()
                end
            end
        end

        showStatus("‚úÖ Game, ReplicatedGUI & decompiler UI copied successfully!")
    end)
    if not success then
        showStatus("‚ùå Failed to copy game: "..tostring(err))
    end
end)

-- Discord Get Key Button
linkButton.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard("https://discord.gg/RyD3MAse")
        showStatus("‚úÖ Discord link copied to clipboard!")
    else
        showStatus("‚ùå setclipboard not supported!")
    end
end)
